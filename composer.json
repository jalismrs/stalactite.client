{
  "name": "jalismrs/stalactite-api.client",
  "license": "proprietary",
  "description": "PHP Client for Stalactite API",
  "type": "library",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "hunomina",
      "email": "hugo.f972@gmail.com"
    }
  ],
  "require": {
    "php": "^7.3",
    "ext-json": "*",
    "hunomina/json-data-validator": "1.10.*",
    "lcobucci/jwt": "3.3.*",
    "symfony/config": ">4.4 <6",
    "symfony/http-client": ">4.4 <6",
    "symfony/property-access": ">4.4 <6",
    "symfony/serializer": ">4.4 <6"
  },
  "require-dev": {
    "phpunit/phpunit": ">7 <9"
  },
  "autoload": {
    "files": [
      "./src/functions.php"
    ],
    "psr-4": {
      "Jalismrs\\Stalactite\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jalismrs\\Stalactite\\Client\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
