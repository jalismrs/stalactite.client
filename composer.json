{
  "name": "jalismrs/stalactite-api.client",
  "license": "proprietary",
  "description": "PHP Client for Stalactite API",
  "type": "library",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "hunomina",
      "email": "hugo.f972@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "hunomina/data-validator": "^4.2",
    "lcobucci/jwt": "^4.0",
    "psr/simple-cache": "^1.0",
    "symfony/config": ">=4.4 <6",
    "symfony/contracts": ">=1.1.7 <3",
    "symfony/http-client": ">=4.4 <6",
    "symfony/property-access": ">=4.4 <6",
    "symfony/serializer": ">=4.4 <6"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Jalismrs\\Stalactite\\Client\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Jalismrs\\Stalactite\\Client\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
